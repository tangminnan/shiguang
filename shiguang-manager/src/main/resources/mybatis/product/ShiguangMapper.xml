    <insert id="save" parameterType="com.shiguang.product.domain.ShiguangDO" useGeneratedKeys="true" keyProperty="id">
		insert into aa_shiguang
		(
			`produc_num`, 
			`produc_code`, 
			`produc_name`, 
			`mfrsid`, 
			`brandid`, 
			`factory`, 
			`produc_factory`, 
			`proid`, 
			`unitid`, 
			`batchid`, 
			`leixing`, 
			`tax`, 
			`tax_price`, 
			`trade_price`, 
			`transfer_price`, 
			`retail_price`
		)
		values
