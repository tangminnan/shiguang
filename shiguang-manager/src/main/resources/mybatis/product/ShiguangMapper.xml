    <select id="get" resultType="com.shiguang.product.domain.ShiguangDO">
		select `id`,`produc_num`,`produc_code`,`produc_name`,`mfrsid`,`brandid`,`factory`,`produc_factory`,`proid`,`unitid`,`batchid`,`leixing`,`tax`,`tax_price`,`trade_price`,`transfer_price`,`retail_price` from aa_shiguang where id = #{value}
	</select>

    <select id="list" resultType="com.shiguang.product.domain.ShiguangDO">
