			`phone_2`,
			`phone_3`,
			`province_code`,
			`province_name`,
			`city_code`,
			`city_name`,
			`area_code`,
			`area_name`,
			`card_type`,
			`master_card`,
			`minor_name`,
			`crowd_type`,
			`exclusive_customer`,
			`exclusive_customer_id`,
			`identity_type`,
			`identity_id`,
			`vocation`,
			`school`,
			`qq_number`,
			`email`,
			`address`,
			`postal_code`,
			`interest`,
			`source`,
			`integral_pasd`,
			`remark`,
