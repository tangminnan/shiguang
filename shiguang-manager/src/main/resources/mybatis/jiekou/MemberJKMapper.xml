			#{interest},
			#{source},
			#{integralPasd},
			#{remark},
			#{registerTime},
			#{status},
			#{state},
			#{openId}
		)
	</insert>

    <update id="update" parameterType="com.shiguang.jiekou.domain.MemberJKDO">
